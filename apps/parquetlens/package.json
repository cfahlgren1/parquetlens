{
  "name": "parquetlens",
  "version": "0.4.1",
  "description": "A fast, interactive TUI for viewing Parquet files",
  "type": "module",
  "license": "MIT",
  "author": "Caleb Fahlgren",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfahlgren1/parquetlens"
  },
  "keywords": [
    "parquet",
    "viewer",
    "tui",
    "cli",
    "data",
    "arrow"
  ],
  "bin": {
    "parquetlens": "dist/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm -C ../../packages/parquet-reader build && pnpm -C ../../packages/sql build && tsx src/main.ts",
    "build": "tsup",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "lint": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@opentui/core": "^0.1.74",
    "@opentui/react": "^0.1.74",
    "cli-table3": "^0.6.5",
    "react": "^19.2.3"
  },
  "optionalDependencies": {
    "@parquetlens/sql": "^0.1.0"
  },
  "devDependencies": {
    "@parquetlens/parquet-reader": "workspace:*",
    "@types/react": "^19.2.9",
    "vitest": "^4.0.18"
  }
}
